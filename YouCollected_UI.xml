<?xml version="1.0"?>
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Frame name="YouCollected_AlertFrame" hidden="true" virtual="true" movable="true" enableMouse="true">
        <Size x="276" y="109"/>
        <Anchors>
            <Anchor point="CENTER" x="0" y="0"/>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Background">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
                </Texture>
            </Layer>
            <Layer level="BORDER">
                <Texture name="$parentIcon" parentKey="ItemIcon">
                    <Size x="52" y="52"/>
                    <Anchors>
                        <Anchor point="LEFT" x="141" y="-2"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="IconBorder">
                    <Size x="60" y="60"/>
                    <Anchors>
						<Anchor point="CENTER" relativeTo="$parentIcon" relativePoint="CENTER" x="0" y="0"/>
					</Anchors>
                </Texture>
                <FontString parentKey="YouCollectedText" inherits="GameFontNormal" justifyH="LEFT" text="You Collected">
                    <Size x="100" y="16"/>
                    <Anchors>
                        <Anchor point="CENTER" x="-6" y="21"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="ItemName" inherits="GameFontHighlightMed2" justifyH="LEFT" text="Felscorned Grandmaster's Companion">
                    <Size x="164" y="38"/>
                    <Anchors>
                        <Anchor point="CENTER" x="29" y="-9"/>
                    </Anchors>
                    <Color color="EPIC_PURPLE_COLOR"/>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>